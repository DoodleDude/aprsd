httpserver.port = 8081
timezone = Europe/Oslo

user.admin = admin
user.update = la7eca|le2xa|admin|vfs 

#######################################
# APRS IS server to get input from
####################################### 
inetchannel.host = aprs.la3t.no
inetchannel.port = 14580 
inetchannel.on   = true
inetchannel.user = LA3T-1 
inetchannel.pass = 3319 


#######################################
# APRS objects setup
#######################################
#objects.file = objects
objects.mycall = LA3T-1
objects.transmit.period = 240 
objects.forceupdate = true

#######################################
# APRS Messages and commands
#######################################
message.mycall = LA3T-1 
message.auth.key = PolaricHacker 
remotectl.mycall = LA3T-1 
remotectl.on = true


#######################################
# Map trails
#######################################
map.trail.maxAge = 40 
map.trail.maxPause = 20 
map.trail.maxAge.extended = 30
map.trail.maxPause.extended = 20 


#######################################
# Properties of map overlay
#######################################
map.icon.default = sym00.gif
map.label.fontsize  = 16
map.label.textcolor = black
map.label.boxcolor  = #ffff88
map.label.boxcolor3 = #e7f4ff
